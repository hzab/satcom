
<h1> Here are all the users: </h1>
<table class="table table-hover">
<thead>
<tr>
<th> Name
</th>
<th> Phone Number
</th>
<th> Email
</th>
<th> Twitter
</th>
<th> Preferred Contact
</th>
</tr>
</thead>
<% @users.each do |user| %>

<tbody>

  <td>
    <%= user.full_name  %>
  </td>

  <td>
    <%= user.phone_number %>
  </td>

  <td>
    <%= user.email %>
  </td>
    <td>
    <%= user.twitter_handle %>
  </td>

  </td>
    <td>
    <%= user.preferred_contact %>
  </td>

</tbody>



<% end %>
</table>

